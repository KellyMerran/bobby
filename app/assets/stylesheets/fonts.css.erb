@font-face {
  font-family: 'neou';
  src:url('<%= asset_path('neou-thin.ttf') %>');
  src:url('<%= asset_path('neou-thin.ttf?#iefix') %>') format('embedded-opentype'),
 	url('<%= asset_path('neou-thin.ttf') %>') format('truetype'),
    url('<%= asset_path('neou-thin.otf') %>') format('otf');
  font-weight: normal;
  font-style: normal;
}


@font-face {
  font-family: 'neou-bold';
  src:url('<%= asset_path('neou-bold.ttf') %>');
  src:url('<%= asset_path('neou-bold.ttf?#iefix') %>') format('embedded-opentype'),
 	url('<%= asset_path('neou-bold.ttf') %>') format('truetype'),
    url('<%= asset_path('neou-bold.otf') %>') format('otf');
  font-weight: normal;
  font-style: normal;
}
